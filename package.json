{
  "name": "yenten-api-blockchain",
  "version": "1.0.0",
  "description": "Yenten blockchain api client. Get and make Yenten transactions.",
  "main": "dist/yenten-api-client.umd.js",
  "types": "dist/yenten-api-client.d.ts",
  "module": "dist/yenten-api-client.esm.min.js",
  "keywords": [
    "yenten",
    "blockchain",
    "ytn",
    "api",
    "client"
  ],
  "exports": {
    ".": {
      "require": "./dist/yenten-api-client.umd.min.js",
      "import": "./dist/yenten-api-client.esm.min.js"
    }
  },
  "scripts": {
    "docs": "typedoc && touch docs/.nojekyll",
    "build:types": "tsc -t esnext --moduleResolution node -d --emitDeclarationOnly --outFile dist/yenten-api-client.d.ts src/yenten-api-client.ts",
    "build:js": "rollup -c rollup.config.js",
    "build:minjs:umd": "terser dist/yenten-api-client.umd.js --compress --mangle > dist/yenten-api-client.umd.min.js",
    "build:minjs:esm": "terser dist/yenten-api-client.esm.js --compress --mangle > dist/yenten-api-client.esm.min.js",
    "build:minjs": "npm run build:minjs:esm -s && npm run build:minjs:umd -s",
    "build": "npm run build:js -s && npm run build:minjs -s && npm run build:types -s",
    "test": "env TS_NODE_PROJECT=\"tsconfig-test.json\" mocha -r ts-node/register **/test/**/*.test.ts",
    "coverage": "nyc --reporter html --reporter text npm test",
    "pub-local": "npm run build && npm version patch && npm publish --registry http://0.0.0.0:4873 && git push origin && git push origin --tags"
  },
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alkeicam/yenten-api-blockchain"
  },
  "author": "Al Keicam<alkeicam@yahoo.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "0.21.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-commonjs": "10.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^6.1.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.32.1",
    "terser": "^5.3.8",
    "tslib": "^2.0.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "mocha": "8.2.1",
    "nyc": "14.1.x",
    "sinon": "^9.0.0",
    "ts-node": "9.0.0",
    "eslint": "7.12.1",
    "@typescript-eslint/parser": "4.6.1",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@types/mocha": "8.0.3",
    "@types/chai": "4.2.14",
    "@types/chai-as-promised": "7.1.3",
    "@types/sinon": "9.0.8"
  }
}
